.gpi_box{
    position:absolute;
    top:10px;
    left:40px;
    width:310px;
    border-top-left-radius:4px;
    border-top-right-radius:4px;
    background-color:#3362ce;
}
.gpi_title{
    width:100%;
    height:30px;
    background: linear-gradient(0, rgba(1, 45, 168, 1) 0%, rgba(1, 45, 168, 1) 0%, rgba(6, 134, 221, 1) 100%, rgba(6, 134, 221, 1) 100%);
    font-weight: bold;
    line-height: 30px;
    border-top-left-radius:4px;
    border-top-right-radius:4px;
    color: #fff;
    text-align:center;
}
.gpi_list{
    font-size:15px;
    width:100%;
    line-height:30px;
    color:#fff;
}
.gpi_list_title{
    background: linear-gradient(0, rgba(1, 45, 168, 1) 0%, rgba(1, 45, 168, 1) 0%, rgba(6, 134, 221, 1) 100%, rgba(6, 134, 221, 1) 100%);
    padding-left:5px;
    height:30px;
    cursor: pointer;
}
.gpi_list_title_detail{
    float:right;
    margin-right:5px;
    font-size:14px;
    cursor:pointer;
}
.gpi_list_box{
    width:100%;
    background-color:#3362ce;
    overflow:hidden;
    height:0;
}
.gpi_list_box>li{
    width:100%;
    height:30px;
    font-size:14px;
    color:#fff;
    display:flex;
}
.gpi_list_box.gpi_default_open{
    height:auto;
}
.gpi_list_title>img{
    width:20px;
    height:20px;
    vertical-align: middle;
}
.gpi_list_box>li>div{
    width:50%;
    height:30px;
    line-height:30px;
    text-align:center;
}
